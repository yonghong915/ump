<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace和定义的Mapper接口对应，并实现其中的方法 -->
<mapper namespace="com.ump.cust.dao.CustContractDao">
	<resultMap type="com.ump.cust.model.CustContract" id="custContractResultMap" extends="com.ump.core.base.dao.BaseDao.baseResultMap">
		<result column="cust_code" property="custCode" jdbcType="VARCHAR" />
	</resultMap>

	<sql id="sql_select">
		SELECT id,cust_code,
		cust_name,
		cust_desc,
		cust_type,
		status,
		creator,
		create_date,
		updator,
		update_date
	</sql>
	<sql id="base_column_list">
		cust_name
	</sql>
	
	<insert id="insertCust" parameterType="com.ump.cust.model.Cust">
	     INSERT INTO bus_cust_contract(id)
	     VALUES(#{id})
	</insert>

</mapper>